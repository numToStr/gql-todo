{
    "name": "gql-todo",
    "version": "1.0.0",
    "description": "A small todo app build with Graphql",
    "author": "Vikas Raj <vikasraj1911@gmail.com>",
    "main": "bin/www.js",
    "bin": {
        "gql-todo": "www.js"
    },
    "scripts": {
        "start": "NODE_ENV=production node bin/www.js",
        "dev": "NODE_ENV=development nodemon -r dotenv/config bin/www.ts",
        "build": "NODE_ENV=production tsc",
        "gql:init": "graphql-codegen init",
        "gql:generate": "graphql-codegen --config codegen.yml"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vkasraj/gql-todo.git"
    },
    "keywords": [
        "node",
        "express",
        "mongod",
        "mongodb",
        "graphql",
        "graphQl",
        "server",
        "api",
        "rest",
        "rest api",
        "todo",
        "todo mvc"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vkasraj/gql-todo/issues"
    },
    "homepage": "https://github.com/vkasraj/gql-todo#readme",
    "dependencies": {
        "apollo-server": "^2.9.3",
        "bcryptjs": "^2.4.3",
        "graphql": "^14.5.7",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.7.1",
        "nexus": "^0.11.7"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^8.3.4",
        "@types/mongoose": "^5.5.17",
        "@types/node": "^12.7.5",
        "dotenv": "^8.1.0",
        "nodemon": "^1.19.2",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3"
    }
}
